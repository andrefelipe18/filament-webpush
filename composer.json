{
    "name": "andrefelipe18/filament-webpush",
    "description": "Easy way to send web push notifications in filament apps.",
    "keywords": [
        "filament-webpush",
        "laravel",
        "filament-webpush"
    ],
    "homepage": "https://github.com/andrefelipe18/filament-webpush",
    "license": "MIT",
    "authors": [
        {
            "name": "Andr√© Domingues",
            "email": "dominguesstroppa@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0||^11.0||^12.0",
        "laravel-notification-channels/webpush": "^10.2",
        "spatie/laravel-package-tools": "^1.16",
        "filament/filament": "^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "rector/rector": "*"
    },
    "autoload": {
        "psr-4": {
            "FilamentWebpush\\": "src/",
            "FilamentWebpush\\Database\\Factories\\": "database/factories/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilamentWebpush\\FilamentWebpushServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
